[
    {
        "endpoint": "addItem",
        "path": "/addItem",
        "method": "POST",
        "request:data": {
            "foodTitle": {
                "type": "String",
                "max": 30,
                "required": true
            },
            "foodDesc": {
                "type": "String",
                "max": 255,
                "required": true
            },
            "foodCost": {
                "type": "Double",
                "required": true
            },
            "foodPic": {
                "type": "String",
                "max": 255,
                "required": true
            },
            "foodType": {
                "type": "boolean",
                "required": true
            }
        },
        "response": {
            "http-status": 200,
            "response:data": {
                "success": {
                    "flag": true,
                    "message": "Added Successfully!"
                },
                "error": {
                    "flag": false,
                    "data": "Update failed"
                }
            }
        }
    }
]